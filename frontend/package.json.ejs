{
    "name"            : "<%= name %>",
    "description"     : "<%= description %>",
    "version"         : "1.0.0",
    "repository"      : {
        "type" : "git",
        "url"  : "https://github.com/<%= owner %>/<%= name %>"
    },
    "bugs"            : {
        "url" : "https://github.com/<%= owner %>/<%= name %>/issues"
    },
    "type"            : "module",
    "scripts"         : {
        "clean"           : "pnpm dlx rimraf --glob **/node_modules/* pnpm-lock.yaml",
        "reset"           : "pnpm clean && pnpm i && pnpm audit -P",
        "lint"            : "eslint src",
        "pretest"         : "pnpm lint",
        "test"            : "vitest --run",
        "test:ui"         : "vitest --ui",
        "test:ci"         : "cross-env NODE_OPTIONS=--max-old-space-size=1536 vitest --run",
        "build"           : "vite build",
        "start"           : "vite dev --host 0.0.0.0",
        "start:ci"        : "vite start",
        "serve"           : "vite serve",
        "storybook"       : "cross-env STORYBOOK_DISABLE_TELEMETRY=1 storybook dev -p 6006",
        "storybook:build" : "pnpm dlx rimraf --glob ./public/design && cross-env STORYBOOK_DISABLE_TELEMETRY=1 storybook build -o ./public/design"
    },
    "dependencies"    : {
        "@ant-design/icons"                   : "^5.5.2",
        "@contentful/rich-text-html-renderer" : "^17.0.0",
        "@reduxjs/toolkit"                    : "^2.5.0",
        "antd"                                : "^5.23.2",
        "contentful"                          : "^11.4.4",
        "i18next"                             : "^24.2.1",
        "i18next-browser-languagedetector"    : "^8.0.2",
        "react"                               : "^19.0.0",
        "react-dom"                           : "^19.0.0",
        "react-i18next"                       : "^15.4.0",
        "react-redux"                         : "^9.2.0",
        "react-router"                        : "^7.1.3",
        "redux-persist"                       : "^6.0.0",
        "usehooks-ts"                         : "^3.1.0"<% if (dependencies.some(dependency => dependency.tier === 'frontend' && !dependency.devOnly)) { %><% dependencies.filter(dependency => dependency.tier === 'frontend' && !dependency.devOnly).forEach(dependency => { %>,
        "<%= dependency.id ?? dependency.name %>" : "<%= dependency.version === 'Latest' ? 'latest' : dependency.version %>"<% }) %><% } %>
    },
    "devDependencies" : {
        "@eslint/eslintrc"              : "^3.2.0",
        "@eslint/js"                    : "^9.18.0",
        "@storybook/addon-actions"      : "^8.4.7",
        "@storybook/addon-essentials"   : "^8.4.7",
        "@storybook/addon-interactions" : "^8.4.7",
        "@storybook/builder-vite"       : "^8.4.7",
        "@storybook/manager-api"        : "^8.4.7",
        "@storybook/react"              : "^8.4.7",
        "@storybook/react-vite"         : "^8.4.7",
        "@storybook/theming"            : "^8.4.7",
        "@stylistic/eslint-plugin-ts"   : "^2.13.0",
        "@testing-library/jest-dom"     : "^6.6.3",
        "@testing-library/react"        : "^16.1.0",
        "@types/ejs"                    : "^3.1.5",
        "@types/eslint__js"             : "^8.42.3",
        "@types/node"                   : "^22.10.7",
        "@types/react"                  : "^19.0.7",
        "@types/react-dom"              : "^19.0.3",
        "@types/react-test-renderer"    : "^19.0.0",
        "@typescript-eslint/parser"     : "^8.21.0",
        "@vitejs/plugin-react-swc"      : "^3.7.2",
        "@vitest/coverage-v8"           : "^2.1.8",
        "@vitest/ui"                    : "^2.1.6",
        "cross-env"                     : "^7.0.3",
        "eslint"                        : "^9.18.0",
        "eslint-plugin-import"          : "^2.31.0",
        "eslint-plugin-prefer-arrow"    : "^1.2.3",
        "eslint-plugin-react"           : "^7.37.4",
        "eslint-plugin-react-hooks"     : "^5.1.0",
        "eslint-plugin-unicorn"         : "^56.0.1",
        "jsdom"                         : "^26.0.0",
        "prop-types"                    : "^15.8.1",
        "storybook"                     : "^8.4.7",
        "storybook-react-i18next"       : "^3.1.8",
        "typescript"                    : "^5.7.3",
        "typescript-eslint"             : "^8.21.0",
        "vite"                          : "^6.0.10",
        "vitest"                        : "^2.1.8"<% if (dependencies.some(dependency => dependency.tier === 'frontend' && dependency.devOnly)) { %><% dependencies.filter(dependency => dependency.tier === 'frontend' && dependency.devOnly).forEach(dependency => { %>,
        "<%= dependency.id ?? dependency.name %>" : "<%= dependency.version === 'Latest' ? 'latest' : dependency.version %>"<% }) %><% } %>
    }
}
